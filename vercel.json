{
  "version": 2,
  "builds": [
    {
      "src": "vercel-build.sh",
      "use": "@vercel/static-build",
      "config": { "distDir": "control-plane/api/ui" }
    },
    {
      "src": "control-plane/api/rest_api_server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(assets|static)/(.*)",
      "dest": "/control-plane/api/ui/$1/$2"
    },
    {
      "src": "/(.*)",
      "dest": "/control-plane/api/rest_api_server.py"
    }
  ]
}
