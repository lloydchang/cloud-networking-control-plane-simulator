{
  "version": 2,
  "functions": {
    "control-plane/api/rest_api_server.py": {
      "runtime": "@vercel/python@6.1.5",
      "maxDuration": 60,
      "memory": 1024,
      "includeFiles": ["control-plane/api/**"]
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/control-plane/api/rest_api_server.py"
    }
  ]
}
