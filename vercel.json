{
  "version": 2,
  "builds": [
    {
      "src": "vercel-build.sh",
      "use": "@vercel/static-build"
    },
    {
      "src": "control-plane/api/rest_api_server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/control-plane/api/rest_api_server.py"
    }
  ]
}
